<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cainam.tuchat.activity.UserListActivity">

    <include layout="@layout/application_toolbar"
        android:id="@+id/all_users_list_toolbar"
        />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/all_users_list_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/all_users_list_toolbar"
        android:divider="@color/colorAccent">

    </android.support.v7.widget.RecyclerView>

</RelativeLayout>
